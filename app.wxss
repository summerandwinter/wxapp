/**app.wxss**/
@font-face {
  font-family: 'iconfont';  /* project id 295262 */
  src: url('//at.alicdn.com/t/font_ldb92dc0j91qncdi.eot');
  src: url('//at.alicdn.com/t/font_ldb92dc0j91qncdi.eot?#iefix') format('embedded-opentype'),
  url('//at.alicdn.com/t/font_ldb92dc0j91qncdi.woff') format('woff'),
  url('//at.alicdn.com/t/font_ldb92dc0j91qncdi.ttf') format('truetype'),
  url('//at.alicdn.com/t/font_ldb92dc0j91qncdi.svg#iconfont') format('svg');
}
.iconfont {
  font-family:"iconfont" !important;
  font-size:16px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-share:before { content: "\e625"; }
.icon-like:before { content: "\e63d"; }
.icon-liked:before { content: "\e668"; }
.icon-photo:before { content: "\e603"; }
page {
  background-color: #fff;
}

.container {
  padding: 0;
  background-color: #fff;
  box-sizing: border-box;
  min-height: 100%;
  justify-content: space-between;
  font-size: 32rpx;
  font-family: -apple-system-font, Helvetica Neue, Helvetica, sans-serif;
}

.slogan {
  position: fixed;
  font-size: 8pt;
  color: #a8a8a8;
  width: 100%;
  left: 0;
  bottom: 0;
  text-align: center;
  z-index: -1;
}

.loadmore {
  width: 65%;
  margin: 1.5em auto;
  line-height: 1.6em;
  font-size: 14px;
  text-align: center;
}

.loadmore-loading {
  width: 20px;
  height: 20px;
  display: inline-block;
  vertical-align: middle;
  animation: loading 1s steps(12, end) infinite;
  background: transparent url('data:image/svg+xml;base64,') no-repeat;
  background-size: 100%;
}

.loadmore-tips {
  font-family: -apple-system-font, Helvetica Neue, Helvetica, sans-serif;
  display: inline-block;
  vertical-align: middle;
}
.loadmore-line {
    border-top: 1px solid #E5E5E5;
    margin-top: 2.4em;
}
.loadmore-line .loadmore-tips {
    position: relative;
    top: -0.9em;
    padding: 0 .55em;
    background-color: #eee;
    color: #999999;
}
.nodata {
  width: 65%;
  margin: 10em auto;
  line-height: 1.6em;
  font-size: 14px;
  text-align: center;
}

.nodata-tips {
  font-family: -apple-system-font, Helvetica Neue, Helvetica, sans-serif;
  display: inline-block;
  vertical-align: middle;
}
.nodata-line {
    border-top: 1px solid #E5E5E5;
}
.nodata-line .nodata-tips {
    position: relative;
    top: -0.9em;
    padding: 0 .55em;
    background-color: #eee;
    color: #999999;
}

@-webkit-keyframes loading {
  0% {
    -webkit-transform: rotate3d(0, 0, 1, 0deg);
            transform: rotate3d(0, 0, 1, 0deg);
  }
  100% {
    -webkit-transform: rotate3d(0, 0, 1, 360deg);
            transform: rotate3d(0, 0, 1, 360deg);
  }
}
@keyframes loading {
  0% {
    -webkit-transform: rotate3d(0, 0, 1, 0deg);
            transform: rotate3d(0, 0, 1, 0deg);
  }
  100% {
    -webkit-transform: rotate3d(0, 0, 1, 360deg);
            transform: rotate3d(0, 0, 1, 360deg);
  }
}

/**加载动画**/

.data-loading {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  background-color: #fff;
  display: table;
}

.loading-container {
  display: table-cell;
  vertical-align: middle;
}

.sk-wave {
  margin: 0 auto;
  width: 100rpx;
  height: 80rpx;
  text-align: center;
  font-size: 20rpx;
}

.sk-wave .sk-rect {
  background-color: #707070;
  height: 100%;
  width: 12rpx;
  margin: 0 4rpx;
  display: inline-block;
  -webkit-animation: sk-waveStretchDelay 1.2s infinite ease-in-out;
  animation: sk-waveStretchDelay 1.2s infinite ease-in-out;
}

.sk-wave .sk-rect1 {
  -webkit-animation-delay: -1.2s;
  animation-delay: -1.2s;
}

.sk-wave .sk-rect2 {
  -webkit-animation-delay: -1.1s;
  animation-delay: -1.1s;
}

.sk-wave .sk-rect3 {
  -webkit-animation-delay: -1s;
  animation-delay: -1s;
}

.sk-wave .sk-rect4 {
  -webkit-animation-delay: -0.9s;
  animation-delay: -0.9s;
}

.sk-wave .sk-rect5 {
  -webkit-animation-delay: -0.8s;
  animation-delay: -0.8s;
}

@-webkit-keyframes sk-waveStretchDelay {
  0%, 40%, 100% {
    -webkit-transform: scaleY(0.4);
    transform: scaleY(0.4);
  }

  20% {
    -webkit-transform: scaleY(1);
    transform: scaleY(1);
  }
}

@keyframes sk-waveStretchDelay {
  0%, 40%, 100% {
    -webkit-transform: scaleY(0.4);
    transform: scaleY(0.4);
  }

  20% {
    -webkit-transform: scaleY(1);
    transform: scaleY(1);
  }
}
