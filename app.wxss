page {
  background-color: #eee;
}

.container {
  padding: 0;
  background-color: #fff;
  box-sizing: border-box;
  min-height: 100%;
  font-size: 32rpx;
  font-family: -apple-system-font, Helvetica Neue, Helvetica, sans-serif;
}

.slogan {
  position: fixed;
  font-size: 8pt;
  color: #a8a8a8;
  width: 100%;
  left: 0;
  bottom: 0;
  text-align: center;
  z-index: -1;
}

.loadmore {
  width: 65%;
  margin: 1.5em auto;
  line-height: 1.6em;
  font-size: 14px;
  text-align: center;
}

.loadmore-loading {
  width: 20px;
  height: 20px;
  display: inline-block;
  vertical-align: middle;
  animation: loading 1s steps(12, end) infinite;
  background: transparent url('data:image/svg+xml;base64,') no-repeat;
  background-size: 100%;
}

.loadmore-tips {
  font-family: -apple-system-font, Helvetica Neue, Helvetica, sans-serif;
  display: inline-block;
  vertical-align: middle;
}
.loadmore-line {
    border-top: 1px solid #E5E5E5;
    margin-top: 2.4em;
}
.loadmore-line .loadmore-tips {
    position: relative;
    top: -0.9em;
    padding: 0 .55em;
    background-color: #eee;
    color: #999999;
}
.nodata {
  width: 65%;
  margin: 10em auto;
  line-height: 1.6em;
  font-size: 14px;
  text-align: center;
}

.nodata-tips {
  font-family: -apple-system-font, Helvetica Neue, Helvetica, sans-serif;
  display: inline-block;
  vertical-align: middle;
}
.nodata-line {
    border-top: 1px solid #E5E5E5;
}
.nodata-line .nodata-tips {
    position: relative;
    top: -0.9em;
    padding: 0 .55em;
    background-color: #eee;
    color: #999999;
}

@-webkit-keyframes loading {
  0% {
    -webkit-transform: rotate3d(0, 0, 1, 0deg);
            transform: rotate3d(0, 0, 1, 0deg);
  }
  100% {
    -webkit-transform: rotate3d(0, 0, 1, 360deg);
            transform: rotate3d(0, 0, 1, 360deg);
  }
}
@keyframes loading {
  0% {
    -webkit-transform: rotate3d(0, 0, 1, 0deg);
            transform: rotate3d(0, 0, 1, 0deg);
  }
  100% {
    -webkit-transform: rotate3d(0, 0, 1, 360deg);
            transform: rotate3d(0, 0, 1, 360deg);
  }
}

/**加载动画**/

.data-loading {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  background-color: #fff;
  display: table;
}

.loading-container {
  display: table-cell;
  vertical-align: middle;
}

.sk-wave {
  margin: 0 auto;
  width: 100rpx;
  height: 80rpx;
  text-align: center;
  font-size: 20rpx;
}

.sk-wave .sk-rect {
  background-color: #707070;
  height: 100%;
  width: 12rpx;
  margin: 0 4rpx;
  display: inline-block;
  -webkit-animation: sk-waveStretchDelay 1.2s infinite ease-in-out;
  animation: sk-waveStretchDelay 1.2s infinite ease-in-out;
}

.sk-wave .sk-rect1 {
  -webkit-animation-delay: -1.2s;
  animation-delay: -1.2s;
}

.sk-wave .sk-rect2 {
  -webkit-animation-delay: -1.1s;
  animation-delay: -1.1s;
}

.sk-wave .sk-rect3 {
  -webkit-animation-delay: -1s;
  animation-delay: -1s;
}

.sk-wave .sk-rect4 {
  -webkit-animation-delay: -0.9s;
  animation-delay: -0.9s;
}

.sk-wave .sk-rect5 {
  -webkit-animation-delay: -0.8s;
  animation-delay: -0.8s;
}

/** 搜索框 **/
.search-container {
  position: fixed;
  left: 0;
  top: 0;
  z-index: 2;
  width: 100%;
}


.search-bar {
  position: relative;
  padding: 8px 10px;
  display: flex;
  box-sizing: border-box;
  background-color: #fff;
}

.search-bar:before {
  content: " ";
  position: absolute;
  left: 0;
  top: 0;
  right: 0;
  height: 1px;
  border-top: 1px solid #fff;
  color: #d7d6dc;
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
}

.search-bar:after {
  content: " ";
  position: absolute;
  left: 0;
  bottom: 0;
  right: 0;
  height: 1px;
  border-bottom: 1px solid #d7d6dc;
  color: #d7d6dc;
  -webkit-transform-origin: 0 100%;
  transform-origin: 0 100%;
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
}


.search-bar__form {
  position: relative;
  -webkit-box-flex: 1;
  -webkit-flex: auto;
  flex: auto;
  background-color: #efeff4;
}

.search-bar__form:after {
  content: '';
  position: absolute;
  left: 0;
  top: 0;
  width: 200%;
  height: 200%;
  -webkit-transform: scale(0.5);
  transform: scale(0.5);
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
  border-radius: 10px;
  border: 1px solid #e6e6ea;
  box-sizing: border-box;
  background: #fff;
}

.search-bar__box {
  position: relative;
  padding-left: 30px;
  padding-right: 30px;
  height: 100%;
  width: 100%;
  box-sizing: border-box;
  z-index: 1;
}

.search-bar__box .search-bar__input {
  padding: 4px 0;
  width: 100%;
  height: 1.42857143em;
  border: 0;
  font-size: 14px;
  line-height: 1.42857143em;
  box-sizing: content-box;
  background: transparent;
}

.search-bar__box .search-bar__input:focus {
  outline: none;
}

.search-bar__box .icon-search {
  position: absolute;
  left: 10px;
  top: 8px;
}

.search-bar__box .icon-clear {
  position: absolute;
  top: 8px;
  right: 0;
  padding: 0 10px;
}

.search-bar__label {
  position: absolute;
  top: 1px;
  right: 1px;
  bottom: 1px;
  left: 1px;
  z-index: 2;
  border-radius: 3px;
  text-align: center;
  display: flex;
  flex-direction: row; 
  flex-wrap: nowrap; 
  justify-content: center; 
  align-items: center; 
  color: #9b9b9b;
  background: #fff;
}

.search-bar__label text {
  display: inline-block;
  font-size: 14px;
  vertical-align: middle;
}

.search-bar__label .icon-search {
  margin-right: 5px;
}

.search-bar__cancel-btn {
  display: block;
  margin-left: 10px;
  line-height: 28px;
  color: #09bb07;
  white-space: nowrap;
}


@-webkit-keyframes sk-waveStretchDelay {
  0%, 40%, 100% {
    -webkit-transform: scaleY(0.4);
    transform: scaleY(0.4);
  }

  20% {
    -webkit-transform: scaleY(1);
    transform: scaleY(1);
  }
}

@keyframes sk-waveStretchDelay {
  0%, 40%, 100% {
    -webkit-transform: scaleY(0.4);
    transform: scaleY(0.4);
  }

  20% {
    -webkit-transform: scaleY(1);
    transform: scaleY(1);
  }
}
