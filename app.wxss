/**app.wxss**/
@font-face {
  font-family: 'iconfont';  /* project id 295262 */
  src: url('//at.alicdn.com/t/font_vw523l5tarjdobt9.eot');
  src: url('//at.alicdn.com/t/font_vw523l5tarjdobt9.eot?#iefix') format('embedded-opentype'),
  url('//at.alicdn.com/t/font_vw523l5tarjdobt9.woff') format('woff'),
  url('//at.alicdn.com/t/font_vw523l5tarjdobt9.ttf') format('truetype'),
  url('//at.alicdn.com/t/font_vw523l5tarjdobt9.svg#iconfont') format('svg');
}
.iconfont {
  font-family:"iconfont" !important;
  font-size:16px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-share:before { content: "\e608"; }
.icon-like:before { content: "\e63d"; }
.icon-liked:before { content: "\e668"; }
.icon-photo:before { content: "\e603"; }
.icon-more:before { content: "\e625"; }
.icon-submit:before { content: "\e609"; }
.icon-save:before { content: "\e604"; }
.icon-preview:before { content: "\e602"; }
.icon-image:before { content: "\e633"; }
